{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "eqeqeq": "warn",
    "no-empty": "warn",
    "one-var-declaration-per-line": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "func-names": "off"
  },
  "globals": {
    "$": true,
    "jv": true,
    "jvk": true
  }
}